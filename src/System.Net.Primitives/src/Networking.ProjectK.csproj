<?xml version="1.0" encoding="utf-8"?>
<Project ToolsVersion="4.0" xmlns="http://schemas.microsoft.com/developer/msbuild/2003" DefaultTargets="Build">
  <PropertyGroup>
    <DefineConstants>$(DefineConstants);PROJECTK</DefineConstants>
    <IsProjectKLibrary>true</IsProjectKLibrary>
    <IsTestNetLibrary>true</IsTestNetLibrary>
  </PropertyGroup>
  <ItemGroup>
    <Compile Include="ProjectK\System\Net\HostInformation.cs" />
  </ItemGroup>
  <ItemGroup>
    <Reference Include="System.Security.Claims" />
  </ItemGroup>
  <Import Project="Networking.msbuild" />
  <ItemGroup>
    <Reference Include="System.Security.SecureString" />
  </ItemGroup>
  <Import Project="System.Net.Primitives.msbuild" />
  <ItemGroup>
    <Reference Include="System.Threading.ThreadPool">
      <Version>4.0.10.0</Version>
    </Reference>
    <Reference Include="System.Threading.Overlapped" />
    <Reference Include="System.Security.Principal.Windows" />
  </ItemGroup>
  <Import Project="System.Net.Sockets.msbuild" />
  <Import Project="System.Net.NetworkInformation.msbuild" />
  <Import Project="System.Net.NameResolution.msbuild" />
  <Import Project="System.Net.Utilities.msbuild" />
  <Import Project="System.Net.Security.msbuild" />
  <!-- For source files to be shown within the visual tree in Solution Explorer, the items must be 
       included directly in the project file. We have System.IO.FileSystem.targets define the Compile items in a made
       up item called CompileItem and then just include it here. -->
  <ItemGroup>
    <Compile Include="@(CompileItem)" />
  </ItemGroup>
</Project>