<?xml version="1.0" encoding="utf-8"?>
<Project ToolsVersion="4.0" DefaultTargets="Build" xmlns="http://schemas.microsoft.com/developer/msbuild/2003">
  <ItemGroup>
    <CLRTestOtherProject Include="$(MSBuildThisFileFullPath)"/>
    <CLRTestContractReference Include="System.Runtime"/>
    <CLRTestContractReference Include="System.Threading" />
    <CLRTestContractReference Include="System.Threading.Tasks" />
    <CLRTestContractReference Include="System.Diagnostics.Debug" />
    <CLRTestContractReference Include="System.Collections" />
  </ItemGroup>
  <ItemGroup Condition="'$(CLRTestProduct)' == 'ProjectN' Or '$(CLRTestProduct)' == 'NetCoreForCoreCLR'">
    <CLRTestWinMDReference Include="Windows" /> 
    <CLRTestContractReference Include="System.Runtime.WindowsRuntime" />
  </ItemGroup> 
  <ItemGroup Condition="'$(CLRTestProduct)' != 'ProjectN' And '$(CLRTestProduct)' != 'NetCoreForCoreCLR'">
    <CLRTestContractReference Include="System.Net.Sockets">
      <Version>4.0.10.0</Version>
    </CLRTestContractReference>
    <CLRTestContractReference Include="System.Net.NetworkInformation">
      <Version>4.0.10.0</Version>
    </CLRTestContractReference>    
    <CLRTestContractReference Include="System.Resources.ResourceManager" />
    <Compile Include="$(MSBuildThisFileDirectory)\ResourceHelper.cs" />
  </ItemGroup> 
  <PropertyGroup Condition="'$(CLRTestProduct)' == 'ProjectN' Or '$(CLRTestProduct)' == 'NetCoreForCoreCLR'">
    <DefineConstants>$(DefineConstants);PROJECTN</DefineConstants>
  </PropertyGroup>
  <ItemGroup>
    <Compile Include="$(MSBuildThisFileDirectory)\Capability.cs" />
    <Compile Include="$(MSBuildThisFileDirectory)\Resources.cs" />
    <Compile Include="$(MSBuildThisFileDirectory)\TestRequirements.cs" />
    <Compile Include="$(MSBuildThisFileDirectory)\ExceptionAssert.cs" />
    <Compile Include="$(MSBuildThisFileDirectory)\AsyncUtilitiesExtensions.cs" />
    <Compile Include="$(MSBuildThisFileDirectory)\TaskAPMExtensions.cs" />
  </ItemGroup>
</Project>
